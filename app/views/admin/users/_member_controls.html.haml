- content_for :breadcrumbs do
  = breadcrumbs @user
  .crumb.end= section_title   

- content_for :content_navigation do
  %h2 Events
  %ul
    %li{:class => active_for('index')}= icon_link_to "Search", admin_users_path, 'search'
  %h2= @user.name
  %ul
    %li{:class => active_for('show')}= icon_link_to "Detail", admin_user_path(@user), 'circle-zoomin'
    %li{:class => active_for('notes')}= icon_link_to "Notes", notes_admin_user_path(@user), 'comment'
